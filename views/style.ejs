<style>
  head, body, html {
    width: 100%;
    height: 100%;
    margin: 0px;
    padding: 0px;
  }
  body {
    <% var d = new Date(); %>
    <% if (d.getMonth() == 11 && d.getDate() >= 23 && d.getDate() <= 25) { %>
      background: linear-gradient(120deg, rgba(255, 0, 0, 0.5) 5%, rgba(0, 255, 0, 0.5));
    <% } else { %>
      background: linear-gradient(60deg, rgba(255, 165, 150, 0.5) 5%, rgba(0, 228, 255, 0.35));
    <% } %>
    background-repeat: no-repeat;
    background-attachment: fixed;
    position: static;
  }
  div {
    margin: 0px;
    padding: 0px;
    background-repeat: no-repeat;
    font-family: 'Ubuntu', sans-serif;
  }
  iframe {
    margin: auto !important;
    padding: 0px;
    width: 450px;
    height: 253.8px;
    margin-bottom: 50px;
    border: 0;
    border-radius: 20px;
  }
  .iframe_cover {
    margin: 0px;
    margin-top: 50px;
    margin-bottom: 50px;
    padding: 0px;
    overflow: hidden;
    border: 0;
    display: inline-block;
  }
  .outter {
    position: absolute;
    display: table;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
  }
  .inside_outter {
    display: table;
    width: 100%;
    height: 500px;
  }
  .inner {
    display: table-cell;
    text-align: center;
    vertical-align: middle;
  }
  .inside_inner {
    display: inline-block;
    text-align: center;
    vertical-align: middle;
  }
  @media (min-width: 1061px) {
    .container {
      display: inline-block;
      width: 1000px;
      background-color: rgba(255, 255, 255, .7);
      border: 0;
      border-radius: 2px;
      padding: 30px;
    }
  } @media (max-width: 1060px) {
    .container {
      display: inline-block;
      width: calc(100% - 60px);
      height: calc(100% - 5px);
      background-color: rgba(255, 255, 255, .7);
      border: 0;
      padding: 30px;
    }
  } @media (max-width: 520px) {
    .episodeContainer {
      display: inline-block;
      width: 460px !important;
      height: auto !important;
      background-color: rgba(255, 255, 255, .7);
      border: 0;
      padding: 30px;
    }
  }
  .element, .element:active {
    width: 200px;
    height: 200px;
    display: inline-block;
    border: 0;
    border-radius: 2px;
  }
  .element {
    color: rgba(0, 0, 0, 0);
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
  }
  .text {
    width: 200px;
    height: 200px;
    vertical-align: middle;
    display: table-cell;
  }
  .child {
    border-radius: 2px;
    background-size: 100%;
    margin: 2px auto;
  }
  .element:hover, .parent:hover, .child:hover {
    background-image: none !important;
    color: rgba(255, 255, 255, 1);
    border-radius: 2px;
  }
  .footer_table {
    display: table;
    width: 100%;
    height: 100px;
    margin-top: 50px;
  }
  .footer_hover {
    display: table-cell;
    text-align: center;
    vertical-align: middle;
    width: 100%;
    height: 100px;
  }
  .footer {
    display: inline-block;
    color: rgba(0, 0, 0, 0);
    width: 100px;
    height: 100px;
    top: 0;
    left: 0;
    background-image: url('/img/MLP_button_TH.png');
    background-size: 100%;
    text-align: center;
    vertical-align: middle;
    line-height: 100px;
    border: 0;
    border-radius: 2px;
    background-color: #B3E7F6;
    cursor: pointer;
  }
  .songs {
    display: inline-block;
    color: rgba(0, 0, 0, 0);
    width: 100px;
    height: 100px;
    top: 0;
    left: 0;
    background-image: url('/img/MLP_SONG.png');
    background-size: 100%;
    text-align: center;
    vertical-align: middle;
    line-height: 100px;
    border: 0;
    border-radius: 2px;
    background-color: #6EC5E9;
  }
  .back {
    display: absolute;
    background: rgba(255, 255, 255, .7);
  }
  .song {
    width: 100%;
    height: 100px;
    display: table;
    border: 0;
    border-radius: 2px;
    background-color: rgb(255, 255, 255);
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
  }
  .inside_song {
    display: table-cell;
    vertical-align: middle;
  }
  @media (min-width: 1061px) {
    .nextElement {
      position: absolute;
      display: inline-block;
      margin-left: 830px;
      margin-top: 130px;
      float: left;
    }
    .inlineNextElement {
      display: none;
    }
    .prevElement {
      position: absolute;
      display: inline-block;
      margin-left: 30px;
      margin-top: 130px;
      float: left;
    }
    .inlinePrevElement {
      display: none;
    }
  } @media (max-width: 1060px) and (min-width: 941px) {
    .nextElement {
      position: absolute;
      display: inline-block;
      right: 0;
      margin-right: 30px;
      margin-top: 130px;
      float: right;
    }
    .inlineNextElement {
      display: none;
    }
    .prevElement {
      position: absolute;
      display: inline-block;
      left: 0;
      margin-left: 30px;
      margin-top: 130px;
      float: left;
    }
    .inlinePrevElement {
      display: none;
    }
  } @media (max-width: 940px) {
    .nextElement {
      display: none;
    }
    .inlineNextElement {
      margin: 2px auto;
      display: inline-block;
    }
    .prevElement {
      display: none;
    }
    .inlinePrevElement {
      margin: 2px auto;
      display: inline-block;
    }
  }
</style>
<link href="https://fonts.googleapis.com/css?family=Ubuntu" rel="stylesheet">
