<!DOCTYPE HTML>
<html>
  <head>
    <meta charset="utf-8">
    <title>WHATOON</title>
    <% include style.ejs %>
  </head>
  <body>
    <div class="outter">
      <div class="inner">
        <div class="container">
          <div class="inside_outter">
            <div class="inner" style="padding-bottom: 0px;">
              <% for (var j = 0; j < data.Property[num].Seasons; j++) { %>
              <div class="child child_<%= data.Property[num].Name %> element" id="<%= data.Property[num].Name %>_<%= data.Property[num].Property[j].Name %>" onclick="window.location='/season?ani=<%= num %>&s=<%= j %>';" style="background-image:url('<%= data.Property[num].Season_Cover %><%= j + 1 %>.png');background-color:#<%= data.Property[num].Property[j].Season_Background %>;background-position:<%= data.Property[num].Property[j].Season_X %>% <%= data.Property[num].Property[j].Season_Y %>%;background-size:<%= data.Property[num].Property[j].Cover_Zoom %>%;width:<%= data.Property[num].Width[j] %>px;height:<%= data.Property[num].Height[j] %>px;"><div class="text" style="width:<%= data.Property[num].Width[j] %>px;height:<%= data.Property[num].Height[j] %>px;"><%= data.Property[num].Title[j] %></div></div>
              <% } %>
              <div class="footer_table">
                <div class="footer_hover">
                  <div class="footer" onclick="window.location='/..';">Home</div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
