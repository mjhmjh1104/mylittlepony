<script type="text/javascript">
  (function ChangeImage() {
    <% for (var i = 0; i < Count; i++) { %>
      <% var rand = Math.floor(Math.random() * Property[i].Cover_Num); %>
    var Id = '<%= Property[i].Name %>';
    document.getElementById(Id).style.backgroundImage = 'url("<%= Property[i].Cover_Img[rand] %>")';
    document.getElementById(Id).style.backgroundSize = '<%= Property[i].Cover_Size[rand] %>% auto';
    document.getElementById(Id).style.backgroundColor = '#<%= Property[i].Cover_Background[rand]%>';
    document.getElementById(Id).style.backgroundPosition = '<%= Property[i].Cover_X[rand] %>% <%= Property[i].Cover_Y[rand] %>%';
    <% } %>
  })();
</script>
